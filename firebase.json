{
  "hosting": {
    "public": "dist/IF105",
    "headers": [ {
      "source" : "**",
      "headers" : [{
        "key" : "Access-Control-Allow-Origin",
        "value" : "*"
      }]
  }],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "hosting": {
      "public": ".",
      "headers": [ {
          "source" : "**",
          "headers" : [{
            "key" : "Access-Control-Allow-Origin",
            "value" : "*"
          }]
      }]
    }
  }
}
